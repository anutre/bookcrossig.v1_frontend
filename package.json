{
  "name": "book-crossing",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run generate:color && ng serve",
    "build": "npm run generate:color && ng build",
    "build:capacitor": "cap add android && cap add ios",
    "open:androin": "npx cap open android",
    "open:ios": "npx cap open ios",
    "cap:sync": "npx cap copy",
    "watch": "ng build --watch --configuration development",
    "stats": "ng build --stats-json",
    "stats:explore": "webpack-bundle-analyzer dist/book-crossing/browser/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "prepare": "husky install",
    "dev:ssr": "ng run book-crossing:serve-ssr",
    "serve:ssr": "node dist/book-crossing/server/main.js",
    "build:ssr": "ng build && ng run book-crossing:server",
    "prerender": "ng run book-crossing:prerender",
    "generate:color": "style-dictionary build book-crossing --config scripts/style-dictionary.config.js && sass3js -f ts src/assets/style/variables/_colors.scss src/assets/style/variables/colors.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.3",
    "@angular/cdk": "^16.1.4",
    "@angular/common": "^16.1.3",
    "@angular/compiler": "^16.1.3",
    "@angular/core": "^16.1.3",
    "@angular/forms": "^16.1.3",
    "@angular/material": "^16.1.4",
    "@angular/platform-browser": "^16.1.3",
    "@angular/platform-browser-dynamic": "^16.1.3",
    "@angular/platform-server": "^16.1.3",
    "@angular/router": "^16.1.3",
    "@capacitor-community/apple-sign-in": "^5.0.0",
    "@capacitor/android": "^4.4.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/core": "^4.4.0",
    "@capacitor/geolocation": "^5.0.6",
    "@capacitor/ios": "^4.4.0",
    "@ngrx/effects": "^16.1.0",
    "@ngrx/router-store": "^16.1.0",
    "@ngrx/store": "^16.1.0",
    "@ngrx/store-devtools": "^16.1.0",
    "@nguniversal/express-engine": "^16.1.1",
    "angular8-yandex-maps": "^16.1",
    "capacitor-plugin-safe-area": "^2.0.5",
    "express": "^4.15.2",
    "google-libphonenumber": "^3.2.31",
    "heic2any": "^0.0.4",
    "intl-tel-input": "^17.0.19",
    "jquery": "^3.6.1",
    "lightgallery": "^2.7.1",
    "ng-multiselect-dropdown": "^0.3.9",
    "ngx-bootstrap": "^10.2.0",
    "ngx-infinite-scroll": "^16.0.0",
    "ngx-intl-tel-input": "^3.2.0",
    "ngx-slick-carousel": "^0.6.0",
    "ngx-toastr": "^17.0.2",
    "ngx-uploader": "^11.0.0",
    "rxjs": "~7.5.0",
    "slick-carousel": "^1.8.1",
    "tslib": "^2.3.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "^16.2.0",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.1.4",
    "@angular/compiler-cli": "^16.1.3",
    "@capacitor/cli": "^4.4.0",
    "@ngrx/eslint-plugin": "^16.1.0",
    "@nguniversal/builders": "^16.1.1",
    "@types/express": "^4.17.0",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/intl-tel-input": "^17.0.6",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^14.15.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.30",
    "prettier": "^2.7.1",
    "sass3js": "^1.0.9",
    "style-dictionary": "^3.8.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~4.9.5"
  },
  "browser": {
    "cluster": false
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
